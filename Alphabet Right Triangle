import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);

       int n=sc.nextInt();
        int i,j;
        for(i=0;i<n;i++){
            char alp=(char)('A'+i);
            for (j=0;j<=i;j++){
                System.out.print((char)(alp+j)+" ");
        
            }
            System.out.println( );
        }
    }
}
